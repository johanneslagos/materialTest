<?xml version="1.0" encoding="utf-8"?>
<manifest package="no.shortcut.materialtest"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".main.LastfmApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".main.ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".main.ui.activity.AlbumDetailActivity"
            android:label="@string/title_activity_album_detail"
            android:theme="@style/AppTheme">
        </activity>
    </application>

</manifest>